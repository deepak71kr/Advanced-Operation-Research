#include <iostream>
#include <vector>
using namespace std;

int main() {
    int B = 8;           
    int c[2] = {4, 6};      
    int a[2] = {2, 1};    

    vector<int> f1(B + 1, 0);
    vector<int> f2(B + 1, 0);

    for (int b = 0; b <= B; b++) {
        int best = 0;
        for (int x1 = 0; a[0] * x1 <= b; x1++) {
            best = max(best, c[0] * x1);
        }
        f1[b] = best;
    }

    for (int b = 0; b <= B; b++) {
        int best = 0;
        for (int x2 = 0; a[1] * x2 <= b; x2++) {
            best = max(best, c[1] * x2 + f1[b - a[1] * x2]);
        }
        f2[b] = best;
    }

    cout << "Maximum value of Z = " << f2[B] << endl;

    return 0;
}
