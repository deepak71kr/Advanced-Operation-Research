#include <iostream>
#include <vector>
using namespace std;

int main() {
    int B = 10;              
    int c[3] = {3, 5, 4};         
    int a[3] = {1, 2, 1};          

    vector<int> f1(B + 1, 0);
    vector<int> f2(B + 1, 0);
    vector<int> f3(B + 1, 0);

    for (int b = 0; b <= B; b++) {
        f1[b] = c[0] * (b / a[0]);
    }
    
    for (int b = 0; b <= B; b++) {
        int best = 0;
        for (int x2 = 0; a[1] * x2 <= b; x2++) {
            best = max(best, c[1] * x2 + f1[b - a[1] * x2]);
        }
        f2[b] = best;
    }

    for (int b = 0; b <= B; b++) {
        int best = 0;
        for (int x3 = 0; a[2] * x3 <= b; x3++) {
            best = max(best, c[2] * x3 + f2[b - a[2] * x3]);
        }
        f3[b] = best;
    }

    cout << "Maximum value of Z = " << f3[B] << endl;

    return 0;
}
